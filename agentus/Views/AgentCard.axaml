<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="1920" Height="1080"
        WindowStartupLocation="CenterScreen"
        x:Class="agentus.Views.AgentCard"
        x:CompileBindings="False"
        Title="AgentCard">
    <Grid ColumnDefinitions=".1* .1* .1*" RowDefinitions=".3* .1* .1*" ShowGridLines="True">
       <StackPanel Name="SP" DataContext="{Binding Agent}">
           <Image Source="{Binding AgentPhoto}"/> 
       </StackPanel>
    </Grid>
</Window>
